{
    "include": ["./src/**/*.ts", "./src/**/*.tsx", "./src/**/*.js"],
    "compilerOptions": {
        "moduleResolution": "Bundler",
        "module": "ES2022",
        "jsx": "react-jsx",
        "target": "ES2022",
        "strict": true,
        "checkJs": true
    },
    "exclude": ["node_modules"]
}
